   "source": [
    "from delta.tables import *\n",
    "\n",
    "columns = [\"language\", \"num_speakers\"]\n",
    "data = [(\"English\", \"1.5\"), (\"Mandarin\", \"1.1\"), (\"Hindi\", \"0.6\")]\n",
    "rdd = spark.sparkContext.parallelize(data)\n",
    "df = rdd.toDF(columns)\n",
    "\n",
    "df.write.format(\"parquet\").save(\"tmp/lake1\")"
